<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="activePanelContainer" prefHeight="663.0" prefWidth="957.0" stylesheets="@../application.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.NotificationWindowController">
   <children>
      <Label fx:id="notificationTable" layoutX="206.0" layoutY="46.0" prefHeight="74.0" prefWidth="558.0" styleClass="MainLabel" text="Notifications Center" />
      <ToggleButton fx:id="ureadOnlyBtn" layoutX="440.0" layoutY="593.0" mnemonicParsing="false" onMousePressed="#Unreadonly" prefHeight="48.0" prefWidth="146.0" styleClass="pressbtn" text="Unread only" />
      <TableView fx:id="notificationsCenterTable" layoutX="10.0" layoutY="119.0" onMousePressed="#setStatus" prefHeight="458.0" prefWidth="936.0">
        <columns>
          <TableColumn fx:id="notificationNumber" prefWidth="86.0" text="Number" />
          <TableColumn fx:id="sendFrom" prefWidth="118.0" text="Send from" />
            <TableColumn fx:id="content" minWidth="0.0" prefWidth="646.0" text="Content" />
            <TableColumn fx:id="status" prefWidth="82.0" text="Status" />
        </columns>
      </TableView>
      <Label fx:id="noNotifiLabel" layoutX="74.0" layoutY="248.0" prefHeight="154.0" prefWidth="834.0" styleClass="MainLabel" text="There are no new notifications " textFill="#e40707" />
      <Label fx:id="allreadyReadLabel" layoutX="70.0" layoutY="601.0" prefHeight="32.0" prefWidth="350.0" text="**This notofication is allready read" textFill="#fc0000">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
