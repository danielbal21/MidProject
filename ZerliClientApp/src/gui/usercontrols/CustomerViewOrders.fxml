<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="663.0" prefWidth="957.0" stylesheets="@../application.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CustomerViewOrdersController">
   <children>
      <Label layoutX="343.0" layoutY="14.0" styleClass="MainLabel" text="My orders" />
      <Button layoutX="9.0" layoutY="594.0" mnemonicParsing="false" onAction="#backPressed" text="Back">
         <styleClass>
            <String fx:value="Left-btn" />
            <String fx:value="pressbtn" />
         </styleClass>
      </Button>
      <TableView fx:id="OrderViewTable" layoutX="51.0" layoutY="101.0" onMouseClicked="#clickOnItem" prefHeight="423.0" prefWidth="856.0">
        <columns>
          <TableColumn fx:id="OrderNumberCol" prefWidth="88.0" text="Number" />
            <TableColumn fx:id="OrderDateCol" prefWidth="255.0" text="Order date" />
            <TableColumn fx:id="shippingDateCol" prefWidth="217.0" text="Supply date" />
            <TableColumn fx:id="priceCol" prefWidth="90.0" text="Price" />
            <TableColumn fx:id="statusCol" prefWidth="200.0" text="Status" />
        </columns>
      </TableView>
      <Label layoutX="265.0" layoutY="533.0" text="Press twice to see full info" textAlignment="CENTER">
         <font>
            <Font name="Maiandra GD" size="38.0" />
         </font></Label>
   </children>
</AnchorPane>
